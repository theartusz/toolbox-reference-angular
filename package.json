{
  "name": "angular-boilerplate",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "node_modules/@angular/cli/bin/ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "lint": "ng lint",
    "test": "ng test",
    "citest": "true || ng test --browsers PhantomJS --code-coverage=true --watch=false",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e --proxy-config proxy.conf.json",
    "build": "ng build --prod --aot=true --source-map=false --build-optimizer=false",
    "license-checker": "npx license-checker --summary --production --excludePackages 'angular-boilerplate@0.0.0' --onlyAllow 'MIT;ISC;Apache-2.0;Apache 2.0;BSD;WTFPL;Artistic-2.0;CC0-1.0;CC-BY-3.0;Unlicense;Public Domain;'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.5",
    "@angular/cdk": "^8.1.4",
    "@angular/common": "^8.2.5",
    "@angular/compiler": "^8.2.5",
    "@angular/core": "^8.2.5",
    "@angular/forms": "^8.2.5",
    "@angular/platform-browser": "^8.2.5",
    "@angular/platform-browser-dynamic": "^8.2.5",
    "@angular/router": "^8.2.5",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "ajv": "8.10.0",
    "core-js": "^2.6.5",
    "rxjs": "6.6.7",
    "rxjs-compat": "^6.4.0",
    "tslib": "1.14.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.3",
    "@angular-devkit/core": "^7.3.5",
    "@angular-devkit/schematics": "7.3.5",
    "@angular/cli": "^8.3.3",
    "@angular/compiler-cli": "^8.2.5",
    "@angular/language-service": "^8.2.5",
    "@types/jasmine": "3.6.10",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "11.15.53",
    "codelyzer": "^5.0.1",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "karma-phantomjs-launcher": "^1.0.4",
    "protractor": "~5.4.2",
    "puppeteer": "^1.20.0",
    "ts-node": "~8.0.3",
    "tslint": "5.20.1",
    "typescript": "~3.5.3"
  }
}
